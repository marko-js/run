{
  "name": "@marko/run-adapter-static",
  "version": "0.1.6",
  "description": "Preview and deploy @marko/run apps as static files",
  "license": "MIT",
  "homepage": "https://github.com/marko-js/run/tree/main/packages/adapters/static",
  "logo": {
    "url": "https://github.com/marko-js/run/raw/main/assets/marko-run.png"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/marko-js/run.git"
  },
  "author": "Ryan Turnquist <rturnq@gmail.com>",
  "keywords": [
    "marko"
  ],
  "type": "module",
  "scripts": {
    "test": "",
    "build": "tsc -b && tsx scripts/build.ts"
  },
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "require": "./dist/index.cjs"
    }
  },
  "typesVersions": {
    "*": {
      "*": [
        "./dist/index.d.ts"
      ]
    }
  },
  "types": "./dist/index.d.ts",
  "main": "./src/index.ts",
  "files": [
    "dist"
  ],
  "peerDependencies": {
    "@marko/run": "^0"
  },
  "devDependencies": {
    "@types/mocha": "^10.0.6",
    "@types/node": "^20.12.12",
    "@types/serve-static": "^1.15.7",
    "cross-env": "^7.0.3",
    "esbuild": "^0.21.4",
    "mocha": "^10.4.0",
    "mocha-snap": "^5.0.0",
    "prettier": "^3.2.5",
    "tsx": "^4.11.0",
    "typescript": "^5.4.5",
    "vite": "^5.2.12"
  },
  "dependencies": {
    "undici": "^6.18.1"
  }
}
